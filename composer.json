{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "backpack/backupmanager": "^5.0",
        "backpack/crud": "^6.7",
        "backpack/logmanager": "^5.0",
        "backpack/permissionmanager": "^7.2",
        "backpack/pro": "@dev",
        "backpack/theme-tabler": "^1.2",
        "greensms/greensms": "^4.2",
        "laravel-notification-channels/expo": "^1.3",
        "laravel-notification-channels/onesignal": "^2.8",
        "laravel/framework": "^11.45",
        "laravel/socialite": "^5.21",
        "laravel/telescope": "^5.7",
        "laravel/tinker": "^2.9",
        "mobiledetect/mobiledetectlib": "^4.8",
        "propaganistas/laravel-phone": "^6.0",
        "pusher/pusher-php-server": "^7.2",
        "socialiteproviders/apple": "^5.6",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/vkontakte": "^5.0"
    },
    "require-dev": {
        "backpack/generators": "^4.0",
        "darkaonline/l5-swagger": "^9.0",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "url": "https://gitlab.tinderone.ru/libraries/backpack-pro.git",
            "type": "git"
        }
    ]
}
